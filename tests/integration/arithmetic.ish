@Test "Addition" { 123 + 456 == 579 }

@Test "Subtraction" { 123 - 456 == --333 }

@Test "Multiplication" { 123 * 456 == 56088 }

@Test "Division" { 56088 / 123 == 456 }

@Test "Power" { 15 ** 5 == 759375 }

@Test "Float Division" { 15.0 / 2.0 == 7.5 }

@Test "Integer Division" { 15 // 2 == 7 }

@Test "Addition Overflow Is Unit" { 2147483647 + 1 #= () }

@Test "Subtraction Overflow Is Unit" { 2147483647 - --1 #= () }

@Test "Multiplication Overflow Is Unit" { 2147483647 * 2 #= () }

@Test "Division Overflow Is Unit" { --2147483648 / --1 #= () }

@Test "Integer Division Overflow Is Unit" { --2147483648 // --1 #= () }

@Test "Remainder Overflow Is Unit" { --2147483648 % --1 #= () }

@Test "Division By Zero Is Unit" { 2147483647 / 0 #= () }

@Test "Integer Division By Zero Is Unit" { 2147483647 // 0 #= () }

@Test "Remainder By Zero Is Unit" { 2147483647 % 0 #= () }

@Test "Float Division By Zero Is Unit" { 2147483647.0 / 0.0 #= () }

@Test "Float Integer Division By Zero Is Unit" { 2147483647.0 // 0.0 #= () }

@Test "Float Remainder By Zero Is Unit" { 2147483647.0 % 0.0 #= () }

@Test "Float Infinite From Addition Is Unit" { FloatMax + FloatMax #= () }

@Test "Float Infinite From Subtraction Is Unit" { FloatMin - FloatMax #= () }

@Test "Float Infinite From Multiplication Is Unit" { FloatMax * FloatMax #= () }

@Test "Float Infinite From Division Is Unit" { FloatMin / --FloatMinPositive #= () }

@Test "Float Infinite From Power Is Unit" { FloatMax ** FloatMax #= () }

